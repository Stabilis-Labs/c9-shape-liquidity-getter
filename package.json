{
  "name": "@stabilis/conc-liq-calculator",
  "version": "1.0.0",
  "description": "A TypeScript library for calculating redemption values of concentrated liquidity positions on the Radix network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src/**/*.ts --exec \"npx ts-node\" src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/tests/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/tests/**/*.ts",
      "!src/types/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "keywords": [
    "radix",
    "dlt",
    "scrypto",
    "typescript",
    "concentrated-liquidity",
    "amm",
    "defi"
  ],
  "author": "Stabilis Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stabilis/conc-liq-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/stabilis/conc-liq-calculator/issues"
  },
  "homepage": "https://github.com/stabilis/conc-liq-calculator#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.44",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@radixdlt/babylon-gateway-api-sdk": "^1.10.1",
    "decimal.js": "^10.4.3"
  }
}
